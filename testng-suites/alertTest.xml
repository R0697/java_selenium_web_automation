<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Alerts Test Cases">
    <test name="Alerts">
        <classes>
            <class name="test.AlertTestClass">
                <methods>
                    <include name="clickButtonToSeeAlert"/>
                    <include name="verifyAlertWillDisplayAfterFiveSeconds"/>
                    <include name="verifyConfirmBoxWillAppear"/>
                    <include name="verifyPromptWillAppear"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>